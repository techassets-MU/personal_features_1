<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_move_form_inherit_l10n_pe_serie_corr" model="ir.ui.view">
		<field name="name">account.move.form.l10n.pe.serie.corr</field>
		<field name="model">account.move</field>
		<field name="inherit_id" ref="account.view_move_form"/>
		<field name="arch" type="xml">
			<xpath expr="//form//field[@name='name']" position="after">
				<field name="x_serie_correlativo" readonly="1"/>
			</xpath>
		</field>
	</record>

	<record id="view_out_invoice_tree_inherit_l10n_pe_serie_corr" model="ir.ui.view">
		<field name="name">account.move.out_invoice.tree.l10n.pe.serie.corr</field>
		<field name="model">account.move</field>
		<field name="inherit_id" ref="account.view_out_invoice_tree"/>
		<field name="arch" type="xml">
			<xpath expr="//list/field[@name='name']" position="after">
				<field name="x_serie_correlativo" readonly="1"/>
			</xpath>
		</field>
	</record>
</odoo>
